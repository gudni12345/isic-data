@model ISIC_DATA.Models.Users
@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Admin</legend>
<div class="container-fluid">
<ul class="thumbnails">
<li class="span12 clearfix">
  <div class="thumbnail clearfix">
    
    <div class="control-group">
       <label class="control-label">  <b>@Html.DisplayNameFor(model => model.Name):</b>  @Html.DisplayFor(model => model.Name) </label>
    </div>
    
   <div class="control-group">
       <label class="control-label">  <b>Username:</b>  @Html.DisplayFor(model => model.UserName) </label>
    </div>
    

    <div class="control-group">
       <label class="control-label">  <b>Country:</b>  @Html.DisplayFor(model => model.Country.Name) </label>
    </div>
    
    <div class="control-group">
       <label class="control-label">  <b>Email:</b>  @Html.DisplayFor(model => model.UserEmail) </label>
    </div>
    

   </div>
  </li>
 </ul>
 </div>
</fieldset>
<p>
    <p class="btn btn-primary icon  pull-left ">
         @Html.ActionLink("Edit", "EditAdmin", new  { id=Model.Id  } )  </p>  |  <p class="btn btn-primary icon  pull-center "> @Html.ActionLink("Back to List", "Index")</p>
    
</p>
