@model IQueryable<ISIC_DATA.Models.NewsArticle>
@{
    ViewBag.Title = "About";
}

   <section>
      
     @foreach (var ab in Model)
     {
     <article>
        <h1 class="NewsTitle">@ab.Title</h1>

        <p>@Html.Raw(ab.Content)</p>

     </article>
     }           
    
</section>        



        
   


